function kanal($ismi, $linki)
{
$ch = curl_init();
curl_setopt($ch, CURLOPT_URL, $linki);
curl_setopt($ch, CURLOPT_RETURNTRANSFER, 1);
curl_setopt($ch, CURLOPT_REFERER, 'https://yerelmedya.tv');
$html = curl_exec($ch);


$t = explode("\r\n",$html);


foreach ($t as $line) {
$p = strpos($line, "file:");
if ($p > 0)
{
$file = str_replace("file:","",$line);
$file = str_replace("'","",$file);
$file = str_replace(",","",$file);
$file = trim($file);
}
}
$m3u8_satir = "#EXTINF:-1,$ismi\r\n$file\r\n";
return($m3u8_satir);
}


function kanal_io( $ismi, $linki)
{
$html = file_get_contents($linki);
preg_match('/file\s*:\s*"(.*?)"/', $html, $results);




$file=$results[1] . "";


$m3u8_satir = "#EXTINF:-1,$ismi\r\n$file\r\n";
return($m3u8_satir);
}
