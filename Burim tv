        @keyframes flash {
            0% { opacity: 1; }
            50% { opacity: 0; }
            100% { opacity: 1; }
        }

        .flashing-text {
            font-size: 1em;
            
            color: brown;
            animation: flash 1s infinite;
        }
    </style>
